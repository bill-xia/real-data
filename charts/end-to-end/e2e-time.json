{
  "data": {
    "url": "end-to-end.csv"
  },
  "title": "End-to-End Time (data to be filled)",
  "mark": {
    "type": "bar"
  },
  "width": 500,
  "height": 300,
  "encoding": {
    "x": {
      "field": "topo",
      "title": "Topology",
      "type": "nominal",
      "sort": "time",
      "axis": {
        "labelFontSize": 13,
        "labelAngle": 0,
        "titleFontSize": 16
      }
    },
    "y": {
      "field": "time",
      "title": "Congerge Time",
      "stack": false,
      "scale": {
        "type": "log",
        "domain": [
          5,
          10000
        ],
        "clamp": true
      },
      "axis": {
        "values": [
          1,
          10,
          100,
          1000,
          10000
        ],
        "labelFontSize": 13,
        "titleFontSize": 16
      }
    },
    "xOffset": {
      "field": "runtime",
      "sort": [
        "Default-frr",
        "REAL-frr",
        "Batfish"
      ]
    },
    "color": {
      "field": "iter",
      "sort": [
        "Default-frr",
        "REAL-frr",
        "REAL-I-frr",
        "Batfish"
      ],
      "scale": {
        "scheme": "pastel1"
      },
      "legend": {
        "title": "Runtime",
        "labelFontSize": 13,
        "titleFontSize": 16
      }
    }
  }
}